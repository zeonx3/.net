@model IEnumerable<DataSoporte2020.Data.tipoplan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Plantilla.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.desc_plan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.costo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.desc_plan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.costo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idplan }) |
            @Html.ActionLink("Details", "Details", new { id=item.idplan }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idplan })
        </td>
    </tr>
}

</table>
